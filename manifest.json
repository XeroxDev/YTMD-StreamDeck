{
  "Name": "YTMD Connector",
  "Author": "XeroxDev",
  "Description": "This Stream Deck Plugin allows you to control the YouTube Music Desktop App (https://github.com/ytmdesktop/ytmdesktop). If you need help, please visit my repository and open a issue https://github.com/XeroxDev/YTMD-StreamDeck/issues/new/choose",
  "URL": "https://github.com/XeroxDev/YTMD-StreamDeck",
  "Icon": "icons/action-image",
  "Version": "1.5.3",
  "Category": "YTMD Connector",
  "CategoryIcon": "icons/category-icon",
  "SDKVersion": 2,
  "Software": {
    "MinimumVersion": "4.9"
  },
  "OS": [
    {
      "Platform": "mac",
      "MinimumVersion": "10.11"
    },
    {
      "Platform": "windows",
      "MinimumVersion": "10"
    }
  ],
  "Actions": [
    {
      "Icon": "icons/music-play",
      "Name": "Play-Pause",
      "States": [
        {
          "Image": "icons/music-pause",
          "TitleAlignment": "middle",
          "FontSize": "16"
        },
        {
          "Image": "icons/music-play",
          "TitleAlignment": "middle",
          "FontSize": "16"
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Plays or Pauses the current song",
      "UUID": "fun.shiro.ytmdc.play-pause"
    },
    {
      "Icon": "icons/music-next",
      "Name": "Next",
      "States": [
        {
          "Image": "icons/music-next",
          "TitleAlignment": "middle",
          "FontSize": "16"
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Go to next song",
      "UUID": "fun.shiro.ytmdc.next"
    },
    {
      "Icon": "icons/music-prev",
      "Name": "Prev",
      "States": [
        {
          "Image": "icons/music-prev",
          "TitleAlignment": "middle",
          "FontSize": "16"
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Go to prev song",
      "UUID": "fun.shiro.ytmdc.prev"
    },
    {
      "Icon": "icons/like-on",
      "Name": "Like",
      "States": [
        {
          "Image": "icons/like-on",
          "TitleAlignment": "middle",
          "FontSize": "16"
        },
        {
          "Image": "icons/like-off",
          "TitleAlignment": "middle",
          "FontSize": "16"
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Likes current song",
      "UUID": "fun.shiro.ytmdc.like"
    },
    {
      "Icon": "icons/dislike-on",
      "Name": "Dislike",
      "States": [
        {
          "Image": "icons/dislike-on",
          "TitleAlignment": "middle",
          "FontSize": "16"
        },
        {
          "Image": "icons/dislike-off",
          "TitleAlignment": "middle",
          "FontSize": "16"
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Dislikes current song",
      "UUID": "fun.shiro.ytmdc.dislike"
    },
    {
      "Icon": "icons/volume-on",
      "Name": "Mute",
      "States": [
        {
          "Image": "icons/volume-on",
          "TitleAlignment": "middle",
          "FontSize": "16"
        },
        {
          "Image": "icons/volume-mute",
          "TitleAlignment": "middle",
          "FontSize": "16"
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Mutes / unmutes volume",
      "UUID": "fun.shiro.ytmdc.mute"
    },
    {
      "Icon": "icons/volume-down",
      "Name": "Volume Down",
      "States": [
        {
          "Image": "icons/volume-down",
          "TitleAlignment": "middle",
          "FontSize": "16"
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Volume Down",
      "UUID": "fun.shiro.ytmdc.volume-down"
    },
    {
      "Icon": "icons/volume-up",
      "Name": "Volume Up",
      "States": [
        {
          "Image": "icons/volume-up",
          "TitleAlignment": "middle",
          "FontSize": "16"
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Volume Up",
      "UUID": "fun.shiro.ytmdc.volume-up"
    },
    {
      "Icon": "icons/action-image",
      "Name": "Track Info",
      "States": [
        {
          "Image": "icons/action-image",
          "TitleAlignment": "middle",
          "FontSize": "12",
          "FontFamily": "Courier New",
          "FontStyle": "Bold"
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Track Title and Thumbnail",
      "UUID": "fun.shiro.ytmdc.song-info"
    },
    {
      "Icon": "icons/shuffle",
      "Name": "Shuffle",
      "States": [
        {
          "Image": "icons/shuffle"
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Shuffle",
      "UUID": "fun.shiro.ytmdc.shuffle"
    },
    {
      "Icon": "icons/repeat",
      "Name": "Repeat",
      "States": [
        {
          "Image": "icons/repeat"
        }
      ],
      "SupportedInMultiActions": true,
      "Tooltip": "Repeat",
      "UUID": "fun.shiro.ytmdc.repeat"
    }
  ],
  "CodePath": "action.html",
  "PropertyInspectorPath": "property-inspector.html"
}
